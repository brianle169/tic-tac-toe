{
  "env": {
    "browser": true,
    "commonjs": true,
    "es2021": true
  },
  "extends": ["airbnb-base", "prettier"],
  "overrides": [],
  "parserOptions": {
    "ecmaVersion": "latest"
  },
  "rules": {
    "prefer-const": "warn",
    "no-unused-vars": "off",
    "no-plusplus": "off",
    "no-console": "off",
    "no-continue": "off",
    "no-nested-ternary": "off"
  }
}
